{
  "meta": {
    "docsUrl": "https://v2.quasar.dev/quasar-plugins/addressbar-color"
  },

  "injection": "$q.addressbarColor",

  "methods": {
    "set": {
      "desc": "设置地址栏的颜色（对于支持它的浏览器）。",
      "params": {
        "hexColor": {
          "type": "String",
          "desc": "颜色的十六进制格式",
          "required": true,
          "examples": [ "#ff0000" ]
        }
      }
    }
  }
}
