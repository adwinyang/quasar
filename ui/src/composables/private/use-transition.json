{
  "props": {
    "transition-show": {
      "extends": "transition",
      "default": "fade"
    },

    "transition-hide": {
      "extends": "transition",
      "default": "fade"
    },

    "transition-duration": {
      "type": [ "String", "Number" ],
      "desc": "过渡时间（以毫秒为单位，无单位）。",
      "default": 300,
      "examples": [ 500, "1200" ],
      "category": "transition"
    }
  }
}
