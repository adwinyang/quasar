{
  "props": {
    "target": {
      "type": [ "Boolean", "String", "Element" ],
      "desc": "配置一个目标元素来触发组件切换；'true'意味着它启用父DOM元素，'false'意味着它禁止将事件附加到任何DOM元素；通过使用一个字符串（CSS选择器）或一个DOM元素，它将事件附加到指定的DOM元素（如果它存在）。",
      "default": true,
      "values": [ "(Boolean) true", "(Boolean) false", "(CSS selector)", "(DOM Element)" ],
      "examples": [
        ":target=\"false\"",
        ":target=\"$refs.target\"",
        "target=\".my-parent\"",
        "target=\"#target-id\""
      ],
      "category": "behavior"
    },

    "no-parent-event": {
      "type": "Boolean",
      "desc": "跳过对目标DOM元素的附加事件（触发该元素被显示）。",
      "category": "behavior"
    },

    "context-menu": {
      "type": "Boolean",
      "desc": "允许组件表现得像一个上下文菜单，通过鼠标右键（或在手机上长点）打开。",
      "category": "behavior"
    }
  }
}
