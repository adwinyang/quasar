{
  "meta": {
    "docsUrl": "https://v2.quasar.dev/vue-components/floating-action-button"
  },

  "props": {
    "type":{
      "type": "String",
      "desc": "定义按钮的HTML DOM类型",
      "default": "a",
      "values": [
        "a", "submit", "button", "reset"
      ],
      "category": "general"
    },

    "outline": {
      "type": "Boolean",
      "desc": "使用 'outline'(大纲) 设计的Fab按钮",
      "category": "style"
    },

    "push": {
      "type": "Boolean",
      "desc": "使用 'push'(推送)设计的Fab按钮",
      "category": "style"
    },

    "flat": {
      "type": "Boolean",
      "desc": "使用 'flat'(平面) 设计的Fab按钮",
      "category": "style"
    },

    "unelevated": {
      "type": "Boolean",
      "desc": "移除阴影",
      "category": "style"
    },

    "padding": {
      "type": "String",
      "desc": "应用自定义的padding（垂直[水平]）；尺寸以CSS为单位，包括单位名称或标准尺寸名称（none|xs|sm|md|lg|xl）；同时在设置时删除最小宽度和高度",
      "examples": [ "16px", "10px 5px", "2rem", "xs", "md lg" ],
      "category": "style"
    },

    "color": {
      "extends": "color"
    },

    "text-color": {
      "extends": "text-color"
    },

    "glossy": {
      "type": "Boolean",
      "desc": "在按钮上应用光亮的效果",
      "category": "style"
    },

    "external-label": {
      "type": "Boolean",
      "desc": "除了FABs之外，作为外部内容显示标签",
      "category": "style|content"
    },

    "label": {
      "type": [ "String", "Number" ],
      "desc": "当Fab被扩展时将显示的标签",
      "default": "",
      "examples": [ "Button Label" ],
      "category": "content"
    },

    "label-position": {
      "type": "String",
      "desc": "图标周围标签的位置",
      "values": [ "top", "right", "bottom", "left" ],
      "category": "style|content"
    },

    "hide-label": {
      "type": "Boolean",
      "desc": "隐藏标签；在动画中切换标签的可见性时非常有用。",
      "category": "style|content"
    },

    "label-class": {
      "type": [ "Array", "String", "Object" ],
      "desc": "归属于标签容器的类别定义",
      "examples": [
        "my-special-class",
        ":input-class=\"{ 'my-special-class': <condition> }\""
      ],
      "category": "style"
    },

    "label-style": {
      "type": [ "Array", "String", "Object" ],
      "desc": "归属于标签容器的样式定义",
      "examples": [
        "background-color: #ff0000",
        ":input-style=\"{ backgroundColor: '#ff0000' }\""
      ],
      "category": "style"
    },

    "square": {
      "type": "Boolean",
      "desc": "在FAB上应用一个矩形的方面",
      "category": "style"
    },

    "disable": {
      "extends": "disable"
    },

    "tabindex": {
      "extends": "tabindex"
    }
  }
}
