{
  "meta": {
    "docsUrl": "https://v2.quasar.dev/vue-components/table"
  },

  "props": {
    "props": {
      "type": "Object",
      "desc": "QTable的列范围的插槽属性",
      "examples": [ ":props=\"props\"" ],
      "category": "general"
    },

    "auto-width": {
      "type": "Boolean",
      "desc": "试图缩小列的宽度大小；对带有复选框/收音机/切换的列很有用。",
      "category": "content"
    },

    "no-hover": {
      "type": "Boolean",
      "desc": "禁用悬停效果",
      "category": "style"
    }
  },

  "slots": {
    "default": {
      "extends": "default"
    }
  }
}
