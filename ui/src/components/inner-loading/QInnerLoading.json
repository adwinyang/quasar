{
  "mixins": [ "composables/private/use-size", "composables/private/use-transition" ],

  "meta": {
    "docsUrl": "https://v2.quasar.dev/vue-components/inner-loading"
  },

  "props": {
    "showing": {
      "type": "Boolean",
      "desc": "状态--是否加载",
      "category": "state"
    },

    "color": {
      "extends": "color",
      "desc": "Quasar调色板中用于内部旋转器的组件的颜色名称（除非使用默认插槽）。"
    },

    "size": {
      "desc": "以CSS为单位的尺寸，包括单位名称，或标准尺寸名称（xs|sm|md|lg|xl），用于内部旋转器（除非使用默认插槽）。",
      "default": "42px"
    },

    "label": {
      "type": "String",
      "desc": "添加一个标签；当使用默认插槽时，会被覆盖。",
      "examples": [ "Please wait..." ],
      "category": "label",
      "addedIn": "v2.2"
    },

    "label-class": {
      "type": "String",
      "desc": "为标签添加CSS类；只沿 'label'(标签) 属性工作。",
      "examples": [
        "text-red q-mt-xl"
      ],
      "category": "label",
      "addedIn": "v2.2"
    },

    "label-style": {
      "type": [ "Array", "String", "Object" ],
      "desc": "对标签应用自定义样式；只沿 'label'(标签) 属性工作。",
      "examples": [
        "font-size: 28px",
        ":label-style=\"{ color: '#ff0000' }\""
      ],
      "category": "label",
      "addedIn": "v2.2"
    },

    "dark": {
      "extends": "dark"
    }
  },

  "slots": {
    "default": {
      "desc": "默认插槽用于替换默认的Spinner；建议：一个Spinner或文本"
    }
  }
}
