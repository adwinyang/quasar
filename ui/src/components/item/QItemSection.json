{
  "meta": {
    "docsUrl": "https://v2.quasar.dev/vue-components/list-and-list-items"
  },

  "props": {
    "avatar": {
      "type": "Boolean",
      "desc": "渲染一个头像项目的侧面（不需要设置 'side' 属性）。",
      "category": "content"
    },

    "thumbnail": {
      "type": "Boolean",
      "desc": "渲染一个缩略图项目的侧面（不需要设置 'side' 属性）。",
      "category": "content"
    },

    "side": {
      "type": "Boolean",
      "desc": "渲染为项目的一个侧面",
      "category": "content"
    },

    "top": {
      "type": "Boolean",
      "desc": "将内容置于顶部（对多行项目有用）。",
      "category": "content"
    },

    "no-wrap": {
      "type": "Boolean",
      "desc": "不要包住文本（对项目的主要内容很有用）",
      "category": "content"
    }
  },

  "slots": {
    "default": {
      "desc": "本节的实际内容"
    }
  }
}
