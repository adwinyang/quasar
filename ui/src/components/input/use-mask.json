{
  "props": {
    "mask": {
      "type": "String",
      "desc": "自定义掩码或预定义掩码名称之一",
      "examples": [
        "###-##", "date", "datetime", "time", "fulltime", "phone", "card"
      ],
      "category": "behavior"
    },

    "fill-mask": {
      "type": [ "Boolean", "String" ],
      "desc": "用指定的字符（如果值不是字符串，则用下划线）填充字符串，以填补掩码的长度。",
      "examples": [ "true", "'0'", "'_'" ],
      "category": "behavior"
    },

    "reverse-fill-mask": {
      "type": "Boolean",
      "desc": "从面具的右侧填充字符串",
      "category": "behavior"
    },

    "unmasked-value": {
      "type": "Boolean",
      "desc": "模型将被解密（不会包含令牌/分隔符）。",
      "category": "behavior"
    }
  }
}
