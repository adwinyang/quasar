{
  "mixins": [ "composables/private/use-size" ],

  "meta": {
    "docsUrl": "https://v2.quasar.dev/vue-components/avatar"
  },

  "props": {
    "font-size": {
      "type": "String",
      "desc": "内容（图标、文本）在CSS单位中的大小，包括单位名称。",
      "examples": [ "18px", "2rem" ],
      "category": "style"
    },

    "color": {
      "extends": "color"
    },

    "text-color": {
      "extends": "text-color"
    },

    "icon": {
      "extends": "icon"
    },

    "square": {
      "extends": "square"
    },

    "rounded": {
      "extends": "rounded"
    }
  },

  "slots": {
    "default": {
      "desc": "可选的；建议：一个字符的字符串，<img>标签"
    }
  }
}
