{
  "mixins": [ "composables/private/use-size" ],

  "meta": {
    "docsUrl": "https://v2.quasar.dev/vue-components/linear-progress"
  },

  "props": {
    "value": {
      "type": "Number",
      "desc": "进度值（0.0 < x < 1.0）。",
      "default": 0,
      "examples": [ ":value=\"0.28\"" ],
      "category": "model"
    },

    "buffer": {
      "type": "Number",
      "desc": "可选的缓冲值（0.0 < x < 1.0）。",
      "examples": [ ":buffer=\"0.78\"" ],
      "category": "behavior"
    },

    "color": {
      "extends": "color"
    },

    "track-color": {
      "extends": "color",
      "desc": "Quasar调色板上的组件轨道的颜色名称"
    },

    "dark": {
      "extends": "dark"
    },

    "reverse":  {
      "type": "Boolean",
      "desc": "反向推进的方向",
      "category": "behavior"
    },

    "stripe": {
      "type": "Boolean",
      "desc":  "绘制条纹；仅用于确定的状态（出于性能原因）",
      "category": "content"
    },

    "indeterminate": {
      "type": "Boolean",
      "desc": "将组件放入不确定模式",
      "category": "behavior"
    },

    "query": {
      "type": "Boolean",
      "desc": "将组件放入查询模式",
      "category": "behavior"
    },

    "rounded": {
      "extends": "rounded"
    },

    "instant-feedback": {
      "type": "Boolean",
      "desc": "模式改变时没有过渡",
      "category": "behavior"
    },

    "animation-speed": {
      "extends": "animation-speed",
      "default": 300,
      "addedIn": "v2.3"
    }
  },

  "slots": {
    "default": {
      "desc": "建议：QTooltip"
    }
  }
}
