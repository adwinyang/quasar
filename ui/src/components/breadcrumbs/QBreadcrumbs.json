{
  "meta": {
    "docsUrl": "https://v2.quasar.dev/vue-components/breadcrumbs"
  },

  "props": {
    "separator": {
      "type": "String",
      "desc": "用来分隔面包屑的字符串",
      "default": "/",
      "examples": [ "-", "|", ">" ],
      "category": "content"
    },

    "active-color": {
      "extends": "color",
      "desc": "活动面包屑的颜色，可以是Quasar调色板中的任何颜色。",
      "default": "primary",
      "category": "style"
    },

    "gutter": {
      "type": "String",
      "desc": "沟插槽值允许你控制面包屑元素之间的空间。",
      "default": "sm",
      "values": [ "none", "xs", "sm", "md", "lg", "xl" ],
      "examples": [ "md", "lg" ],
      "category": "content"
    },

    "separator-color": {
      "extends": "color",
      "desc": "用于给分离器着色的颜色，可以是Quasar调色板中的任何颜色。",
      "category": "style"
    },

    "align": {
      "type": "String",
      "default": "left",
      "desc": "指定如何水平对齐面包屑",
      "values": [ "left", "center", "right", "between", "around", "evenly" ],
      "examples": [ "left", "center" ],
      "category": "content"
    }
  },

  "slots": {
    "default": {
      "extends": "default"
    },

    "separator": {
      "desc": "你可以插入HTML或组件来分离面包屑"
    }
  }
}
