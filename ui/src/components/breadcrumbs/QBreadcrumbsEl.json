{
  "meta": {
    "docsUrl": "https://v2.quasar.dev/vue-components/breadcrumbs"
  },

  "mixins": [ "composables/private/use-router-link" ],

  "props": {
    "label": {
      "type": "String",
      "desc": "面包屑的标签文本",
      "examples": [ "Home", "Index" ],
      "category": "content"
    },

    "icon": {
      "extends": "icon"
    },

    "tag": {
      "extends": "tag",
      "default": "span",
      "examples": [ "div", "span" ]
    },

    "ripple": {
      "extends": "ripple"
    }
  },

  "slots": {
    "default": {
      "desc": "这是自定义内容的地方，除非 'icon'(图标) 和 'label'(标签) 属性不够用。"
    }
  }
}
