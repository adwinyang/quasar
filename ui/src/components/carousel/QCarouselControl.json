{
  "meta": {
    "docsUrl": "https://v2.quasar.dev/vue-components/carousel"
  },

  "props": {
    "position": {
      "type": "String",
      "desc": "要坚持的侧面/角落",
      "default": "bottom-right",
      "values": [
        "top-right", "top-left",
        "bottom-right", "bottom-left",
        "top", "right", "bottom", "left"
      ],
      "examples": [ "top-right" ],
      "category": "position"
    },

    "offset": {
      "type": "Array",
      "desc": "一个由两个数字组成的数组，用于在水平和垂直方向上偏移组件（单位：像素）。",
      "default": "[18, 18]",
      "examples": [ "[8, 8]", "[5, 10]" ],
      "category": "position"
    }
  },

  "slots": {
    "default": {
      "extends": "default"
    }
  }
}
