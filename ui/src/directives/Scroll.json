{
  "meta": {
    "docsUrl": "https://v2.quasar.dev/vue-directives/scroll"
  },

  "value": {
    "type": "Function",
    "desc": "发生滚动时调用的函数（使用非函数来禁用）。",
    "params": {
      "verticalScrollPosition": {
        "type": "Number",
        "desc": "从顶部滚动偏移",
        "examples": [ 350 ]
      },
      "horizontalScrollPosition": {
        "type": "Number",
        "desc": "从左边开始滚动偏移",
        "examples": [ 350 ]
      }
    },
    "returns": null,
    "examples": [ "v-scroll=\"fnToCall\"", "v-scroll=\"void 0\"" ]
  }
}
