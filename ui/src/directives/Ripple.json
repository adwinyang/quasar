{
  "meta": {
    "docsUrl": "https://v2.quasar.dev/vue-directives/material-ripple"
  },

  "quasarConfOptions": {
    "propName": "ripple",
    "definition": {
      "early": {
        "type": "Boolean",
        "desc": "在用户互动时及早/立即触发"
      },

      "stop": {
        "type": "Boolean",
        "desc": "停止点击/触摸事件的传播"
      },

      "center": {
        "type": "Boolean",
        "desc": "涟漪从绝对中心开始"
      },

      "color": {
        "type": "String",
        "desc": "Quasar调色板的颜色名称；覆盖默认的动态颜色",
        "examples": [ "orange-5" ]
      },

      "keyCodes": {
        "type": [ "Array", "Number" ],
        "desc": "应该触发波纹的keyCode的列表",
        "examples": [ "[]", "[13, 32]" ]
      }
    }
  },

  "value": {
    "type": [ "Boolean", "Object" ],
    "desc": "布尔型（如果只是想启用/禁用）或对象型以配置更多的选项",
    "definition": {
      "early": {
        "type": "Boolean",
        "desc": "在用户互动时及早/立即触发"
      },

      "stop": {
        "type": "Boolean",
        "desc": "停止点击/触摸事件的传播"
      },

      "center": {
        "type": "Boolean",
        "desc": "涟漪从绝对中心开始"
      },

      "color": {
        "type": "String",
        "desc": "Quasar调色板的颜色名称；覆盖默认的动态颜色",
        "examples": [ "orange-5" ]
      },

      "keyCodes": {
        "type": [ "Array", "Number" ],
        "desc": "应该触发波纹的keyCode的列表",
        "examples": [ "[]", "[13, 32]" ]
      }
    },
    "examples": [
      "v-ripple=\"booleanState\"",
      "v-ripple=\"{ center: true, color: 'primary', keyCodes: [] }\""
    ]
  },

  "arg": {
    "type": "String",
    "desc": "Quasar调色板的颜色名称；覆盖默认的动态颜色",
    "examples": [
      "v-ripple:orange-5"
    ]
  },

  "modifiers": {
    "early": {
      "type": "Boolean",
      "desc": "在用户互动时及早/立即触发"
    },

    "stop": {
      "type": "Boolean",
      "desc": "停止点击/触摸事件的传播",
      "examples": [
        "v-ripple.stop"
      ]
    },

    "center": {
      "type": "Boolean",
      "desc": "涟漪从绝对中心开始",
      "examples": [
        "v-ripple.center"
      ]
    }
  }
}
